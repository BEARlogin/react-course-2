{
  "name": "books",
  "version": "1.0.0",
  "source": "src/index.js",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/",
  "module": "./dist/index.module.js",
